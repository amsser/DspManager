                          <select class="form-control" id="ip" name="ip">
                            <option value="122.102.0.0">北京</option>
                            <option value="123.56.128.0">上海</option>
                            <option value="202.38.171.0">广州</option>
                          </select>
                        </div>
                    </div>
                    <span class="help-block">广告展示信息</span>
                    <div class="form-group">
                        <div class="col-md-4 col-sm-offset-1">
                          <label for="select" class="control-label">广告位高*宽</label>
                          <select class="form-control" id="size" name="size">
                            <option value="728*90">728*90</option>
                            <option value="160*600">160*600</option>
                            <option value="250*250">250*250</option>
                            <option value="200*200">200*200</option>
                            <option value="300*250">300*250</option>
                            <option value="960*60">960*60</option>
                            <option value="468*60">468*60</option>
                          </select>
                        </div>
                        <div class="col-md-4 ">
                          <label for="select" class="control-label">广告位位置</label>
                          <select class="form-control" id="pos" name="pos">
                            <option value="0">未知</option>
                            <option value="1">首屏</option>
                            <option value="2">非首屏</option>
                          </select>
