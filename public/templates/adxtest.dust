{>"layouts/master" /}

{<body}
    <div class="row">
        <div class="col-md-2">
            <ul class="nav nav-pills nav-stacked" style="max-width: 150px;">
                <div class="panel panel-default">
                    <div class="panel-heading">京东商城</div>
                    <div class="panel-body">
                        今日花费：{todaypay}
                        账户余额：{balance}
                    </div>
                </div>
            </ul>
        </div>

        <div class="col-md-10">

        <div class="list-group">
            <p class="list-group-item-text">
            <form class="form-horizontal" method="post" action="/adxtest">
                <fieldset>
                    <span class="help-block">广告请求的唯一标识</span>
                    <div class="form-group">
                      <div class="col-md-4 col-sm-offset-1">
                        <label for="textArea" class="control-label">id</label>
                        <input type="text" class="form-control" id="adreqid" name="adreqid">
                      </div>
                    </div>
                    <span class="help-block">网站信息</span>
                    <div class="form-group">
                        <div class="col-md-4 col-sm-offset-1">
                          <label for="select" class="control-label">网站域名</label>
                          <select class="form-control" id="domain" name="domain">
                            <option value="http://a.com">a.com</option>
                            <option value="http://b.com">b.com</option>
                            <option value="http://c.com">c.com</option>
                            <option value="http://d.com">d.com</option>
                            <option value="http://e.com">e.com</option>
                            <option value="http://f.com">f.com</option>
                            <option value="http://g.com">g.com</option>
                            <option value="http://h.com">h.com</option>
                            <option value="http://i.com">i.com</option>
                            <option value="http://j.com">j.com</option>
                          </select>
                        </div>
                    </div>
                    <span class="help-block">浏览器类型</span>
                      <div class="form-group">
