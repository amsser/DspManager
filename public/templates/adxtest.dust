                    <div class="form-group">
                        <div class="col-md-4 col-sm-offset-1">
                          <label for="select" class="control-label">User Id</label>
                          <select class="form-control" id="userid" name="userid">
                            <option value="user001">user001</option>
                            <option value="user002">user002</option>
                            <option value="user003">user003</option>
                            <option value="user004">user004</option>
                            <option value="user005">user005</option>
                            <option value="user006">user006</option>
                            <option value="user007">user007</option>
                            <option value="user008">user008</option>
                            <option value="user009">user009</option>
                            <option value="user010">user010</option>
                            <option value="user011">user011</option>
                            <option value="user012">user012</option>
                            <option value="user013">user013</option>
                            <option value="user014">user014</option>
                            <option value="user015">user015</option>
                            <option value="user016">user016</option>
                            <option value="user017">user017</option>
                            <option value="user018">user018</option>
                            <option value="user019">user019</option>
                            <option value="user020">user020</option>
